{
  "name": "webpackless-react",
  "version": "1.0.0",
  "description": "Broccoli.js react application",
  "main": " ",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && broccoli build dist",
    "build-prod": "npm run clean && BROCCOLI_ENV=production broccoli build dist",
    "serve": "broccoli serve",
    "debug-build": "npm run clean && node $NODE_DEBUG_OPTION $(which broccoli) build dist",
    "debug-serve": "node $NODE_DEBUG_OPTION $(which broccoli) serve"
  },
  "keywords": [
    "broccoli"
  ],
  "author": "Dexter Edwards",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "broccoli": "^1.1.4",
    "broccoli-asset-rev": "^2.6.0",
    "broccoli-clean-css": "^2.0.1",
    "broccoli-cli": "^1.0.0",
    "broccoli-env": "^0.0.1",
    "broccoli-funnel": "^2.0.1",
    "broccoli-lint-eslint": "^3.3.2",
    "broccoli-livereload": "^1.3.0",
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-rollup": "^2.0.0",
    "broccoli-sass-lint": "^1.1.2",
    "broccoli-sass-source-maps": "^2.2.0",
    "broccoli-stew": "^1.5.0",
    "broccoli-uglify-sourcemap": "^2.0.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
